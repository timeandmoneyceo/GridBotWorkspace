def hello_world():
    """An enhanced hello world function"""
    message = "Hello, Enhanced World!"
    print(message)
    return "enhanced_success"

def add_numbers(a, b):
    """Add two numbers with logging"""
    print(f"Adding {a} + {b}")
    return a + b

def multiply_numbers(a, b):
    """Multiply two numbers"""
    return a * b

if __name__ == "__main__":
    hello_world()
    result = add_numbers(5, 3)
    print(f"5 + 3 = {result}")
